CREATE TABLE `users` (
  id int(11) NOT NULL AUTO_INCREMENT,
  username varchar(255) NOT NULL,
  full_name varchar(255) NOT NULL,
  address varchar(255) NOT NULL,
  gender INT NOT NULL,
  email varchar(255) NOT NULL,
  password varchar(64) NOT NULL,
  privilege varchar(30) NOT NULL,
  PRIMARY KEY(id)
);

CREATE TABLE medicines (
    id INT AUTO_INCREMENT PRIMARY KEY,
    name VARCHAR(255) NOT NULL,
    price DECIMAL(10, 2) NOT NULL,
    description TEXT,
    quantity INT NOT NULL,
    file_path VARCHAR(255)
);

CREATE TABLE `medicines` (
  `id` int(11) NOT NULL PRIMARY KEY,
  `name` varchar(255) NOT NULL,
  `price` decimal(10,2) NOT NULL,
  `description` text DEFAULT NULL,
  `quantity` int(11) NOT NULL,
  `file_path` varchar(255) DEFAULT NULL,
  `unit` varchar(255) DEFAULT NULL,
  `komposisi` text DEFAULT NULL,
  `indikasi` text DEFAULT NULL,
  `dosis` text DEFAULT NULL,
  `aturan_pakai` text DEFAULT NULL,
  `efek` text DEFAULT NULL,
  `golongan` text DEFAULT NULL,
  `kemasan` text DEFAULT NULL,
  `manufaktur` text DEFAULT NULL
);

CREATE TABLE carts (
    id INT AUTO_INCREMENT PRIMARY KEY,
    user_id INT NOT NULL,
    medicine_id INT NOT NULL,
    quantity INT NOT NULL,
    FOREIGN KEY (user_id) REFERENCES users(id),
    FOREIGN KEY (medicine_id) REFERENCES medicines(id)
);

CREATE TABLE feedbacks (
    id INT AUTO_INCREMENT PRIMARY KEY,
    fullname varchar(255) NOT NULL,
    user_feedback TEXT NOT NULl,
    file_path VARCHAR(255)
);



INSERT INTO `medicines` (`id`, `name`, `price`, `description`, `quantity`, `file_path`, `unit`, `komposisi`, `indikasi`, `dosis`, `aturan_pakai`, `efek`, `golongan`, `kemasan`, `manufaktur`) VALUES
(1, 'Soricox 120mg Fc Tablet (per Tablet)', '16500.00', 'SORICOX 120MG FC TAB 7S memiliki kandungan bahan aktif Etoricoxib 120 mg digunakan untuk mengatasi nyeri dan pembengkakan akibat rheumatoid arthritis, gout arthritis serta ketika pasien juga mengalami peradangan, dan mengurangi gangguan inflamasi (peradangan) secara umum. Etoricoxib termasuk dalam golongan Nonsteroidal Anti-Inflammatory Drug (NSAID) yang memiliki mekanisme kerja dalam mengatasi nyeri sebagai berikut :\r\n• Menghambat kerja dari enzim siklooksigenasi (COX) dimana enzim ini berfungsi dalam membantu pembentukan prostaglandin saat terjadinya luka dan menyebabkan rasa sakit serta peradangan. Ketika kerja enzim COX terhalangi, maka produksi prostaglandin lebih sedikit, sehingga rasa sakit dan peradangan akan berkurang.\r\n\r\nSORICOX 120MG FC TAB 7S dapat dikonsumsi sebelum atau sesudah makan. Konsultasikan terlebih dahulu kepada dokter apabila akan digunakan pada pasien dengan riwayat atau kondisi sakit seperti:\r\n• Riwayat alergi terhadap kandungan obat ini.\r\n• Pasien dengan penyakit ginjal stadium lanjut.\r\n• Pasien dengan gangguan fungsi ginjal yang signifikan.\r\n• Pasien dengan kondisi gagal hati atau sirosis yang tak terkompensasi.\r\n• Pasien yang sebelumnya memiliki riwayat serangan akut.\r\n\r\nInteraksi dapat terjadi apabila digunakan bersamaan dengan obat-obatan seperti:\r\n• Warfarin.\r\n• Rifampin.\r\n• Metotreksat.\r\n• ACE Inhibitor.\r\n• Aspirin. \r\n\r\n', 100, 'storage/image/aspirin.png', 'Tablet', 'Etoricoxib 120 mg.\r\n\r\n', '• Meredakan gejala pada terapi OA atau Osteoartritis (penyakit degeneratif sendi).\r\n• Meredakan nyeri muskuloskeletal.\r\n• Meredakan nyeri akut yang berhubungan dengan operasi gigi.\r\n\r\n', '• OA atau Osteoartritis (penyakit degeneratif sendi), nyeri muskuloskeletal kronik: 60 mg 1x/hari.\r\n• Nyeri akut yang berhubungan dengan operasi gigi: 120 mg 1x/hari.\r\n\r\n', 'Dapat diberikan sebelum atau sesudah makan.\r\n\r\n', 'rasa lelah, pusing, sakit kepala, edema, rasa panas pada ulu hati, lelah, hipertensi, dispepsia, peningkatan ALT/AST.\r\n\r\n', 'Obat Keras\r\n\r\n', '1 Dos isi 1 Strip x 7 Tablet\r\n\r\n', 'SOHO INDUSTRI PHARMASI'),
(2, 'Amoxicillin Dexa 500mg Tablet (per Tablet)', '526.00', 'AMOXICILLIN DEXA 500MG TAB 100S merupakan obat antibiotik golongan penisilin. Amoxicillin tersedia dalam bentuk tablet maupun sirup. Obat ini menghambat pertumbuhan bakteri yang menyebabkan infeksi dalam tubuh dengan cara menghambat pembentukan dinding sel bakteri. Obat ini, didapatkan hanya dengan resep dokter dan tidak dijual bebas. \r\nAmoxicillin digunakan untuk mengobati infeksi seperti: \r\n1.Bronkitis akut \r\n2.Infeksi THT (telinga, hidung, dan tenggorokan) seperti, -sinus -infeksi telinga luar (otitis eksterna) -infeksi telinga tengah (otitis media) \r\n3.Infeksi saluran kencing \r\n4.Infeksi kulit \r\n5.Infeksi organ kelamin seperti gonore\r\nAmoxicillin dapat dikonsumsi bersamaan dengan makan untuk menghindari efek tidak nyaman pada sistem pencernaan. Amoxicillin harus dikonsumsi sampai habis untuk mencegah resistensi atau keadaan di mana kuman tidak dapat lagi dibunuh dengan obat ini. Penggunaan antibiotik yang berlebihan atau tidak mengikuti anjuran dokter sangatlah berbahaya dan dapat menyebabkan efektivitasnya menurun. Beritahukan dokter yang menangani bila sedang hamil, berencana hamil ataupun sedang menyusui. Hati-hati penggunaan pada penderita yang mempunyai riwayat alergi terhadap antibiotik Amoxicillin karena dapat menimbulkan reaksi pada kulit yang ditandai dengan munculnya bentol berwarna kemerahan disertai rasa gatal, penderita asma, gangguan fungsi ginjal dan hati. Ada beberapa interaksi yang dapat terjadi, bila menggunakan amoxillin bersamaan dengan obat lain, seperti: \r\n1.Meningkatnya efek samping dari obat amoxicillin, jika digunakan dengan obat probenecid. \r\n2.Meningkatnya risiko alergi obat seperti ruam kulit, bila dikonsumsi bersamaan dengan obat allopurinol. \r\n3.Meningkatnya risiko perdarahan jika digunakan bersamaan dengan obat pengencer darah. \r\n4.Menurunkan efektivitas kontrasepsi oral.\r\n\r\n', 50, 'storage/image/amoxicillin.png', 'Tablet', 'Amoxicillin 500 mg\r\n\r\n', 'Amoxicillin digunakan untuk mengobati infeksi telinga tengah, radang tenggorokan, infeksi saluran pernafasan atas, infeksi saluran kemih, saluran cerna, kulit dan jaringan lunak.\r\n\r\n', 'Dewasa: 250-500 mg setiap 8 jam (3x sehari) , Anak-anak: 20-40mg kg/BB/hari dalam 3 dosis terbagi.\r\n\r\n', 'Dapat diberikan bersama makanan agar diabsorpsi lebih baik & untuk mengurangi rasa tidak nyaman pada Gastrointestinal.\r\n\r\n', 'reaksi hipersensitivitas seperti urtikaria, demam nyeri sendi, diare, syok anafilaksis, ruam eritematosus, leukemia limfatik kronik, dan iritasi gastrointestinal.\r\n\r\n', 'Obat Keras\r\n\r\n', '1 Dos isi 10 Strip x 10 Tablet\r\n\r\n', 'DEXA'),
(3, 'Lipitor 80mg Tablet (per Tablet)', '30022.00', 'LIPITOR 80MG TAB 30S memiliki kandungan bahan aktif Atorvastatin. Atorvastatin merupakan obat yang digunakan untuk menurunkan kolesterol jahat (LDL) dan trigliserida serta meningkatkan jumlah kolesterol baik (HDL) di dalam darah. Obat ini bekerja dengan cara menghambat enzim yang memproduksi kolesterol di hati.\r\n\r\nLipitor dapat dikonsumsi sebelum atau sesudah makan. Konsultasikan terlebih dahulu kepada dokter apabila akan digunakan pada pasien dengan riwayat atau kondisi sakit seperti:\r\n• Riwayat alergi terhadap kandungan obat ini.\r\n• Riwayat penyakit hati.\r\n• Pasien dengan gangguan ginjal.\r\n• Gangguan kelenjar tiroid.\r\n\r\nInteraksi dapat terjadi apabila digunakan bersamaan dengan obat-obatan seperti:\r\n• Fenofibrate: Meningkatkan risiko gangguan otot (miopati) dan rhabdomyolysis.\r\n• Rifampin: Menurunkan tingkat atau efek Atorvastatin.\r\n\r\n', 75, 'storage/image/lipitor.png', 'Tablet', 'Atorvastatin 80 mg.\r\n\r\n', 'Menurunkan kolesterol jahat (LDL) dan trigliserida serta meningkatkan jumlah kolesterol baik (HDL) di dalam darah.\r\n\r\n', '• Dewasa: Dosis awal adalah 10-20 mg sekali sehari.\r\n• Anak-anak usia ? 10 tahun: 10 mg sekali sehari.\r\n\r\n', 'Dapat dikonsumsi sebelum atau sesudah makan.\r\n\r\n', 'diare, dispepsia, mual, perut kembung, nyeri muskuloskeletal, kejang otot, mialgia, pembengkakan sendi.\r\n\r\n', 'Obat Keras\r\n\r\n', '1 Dos isi 3 Strip x 10 Tablet\r\n\r\n', 'PFIZER INDONESIA'),
(4, 'Ibuprofen Triman 400mg Tablet (per Tablet)', '414.00', 'IBUPROFEN TRIMAN 400MG TAB 100S termasuk obat golongan nonsteroidal anti-inflammatory drug (NSAID) yang memiliki mekanisme kerja dengan cara menghambat enzim siklooksigenase sehingga konversi asam arakidonat menjadi terganggu serta menghalangi tubuh memproduksi prostaglandin, yaitu senyawa yang menyebabkan peradangan dan rasa sakit serta efektif terhadap nyeri dengan intensitas rendah sampai sedang, dan efektif terhadap nyeri yang berkaitan dengan inflamasi atau kerusakan jaringan seperti:\r\n• Penyakit rematik tulang dan sendi.\r\n• Gejala arthritis, osteoarthritis, dan non-sendi. • Gejala-gejala akibat trauma otot dan tulang atau sendi (trauma muskuloskeletal).\r\n• Nyeri ringan sampai sedang antara lain nyeri pada dismenore primer (nyeri haid), nyeri pada gigi, sakit kepala.\r\n\r\nIbuprofen dapat dikonsumsi bersamaan dengan makanan. Konsultasikan terlebih dahulu kepada dokter apabila akan digunakan pada pasien dengan kondisi:\r\n• Riwayat alergi terhadap kandungan obat ini.\r\n• Penderita riwayat tukak peptik\r\n• Gangguan fungsi ginjal\r\n• Koagulasi darah\r\n• Gagal jantung\r\n• Hipertensi\r\n• Retensi cairan tubuh\r\n• Asma\r\n• Lupus eritematosus sistemik\r\n\r\nInteraksi dapat terjadi apabila digunakan bersamaan dengan obat-obatan seperti:\r\n• Ibuprofen dapat menurunkan efek dari obat golongan ACE inhibitor dan ARB.\r\n• Peningkatan risiko luka atau pendarahan pada saluran pencernaan jika digunakan bersama dengan NSAID lain seperti aspirin, antiplatelet, antikoagulan (misalnya warfarin), kortikosteroid serta SSRI.\r\n\r\n', 120, 'storage/image/ibuprofen.png', 'Tablet', 'Ibuprofen 400 mg.\r\n\r\n', 'Nyeri dengan intensitas rendah sampai sedang seperti sakit kepala, sakit gigi, radang sendi, dan nyeri otot.\r\n\r\n', '• Dewasa 200 mg 3-4x sehari\r\n• Anak 1-12 tahun untuk mengurangi demam pada anak dengan suhu 39 derajat Celcius 10mg/kgBB diberikan 3-4x / hari.\r\n\r\n', 'Dapat diberikan bersamaan dengan makan.\r\n\r\n', 'Mual, muntah, diare, konstipasi, nyeri lambung, ruam kulit, bronkospasme, trombositopenia, limfopenia, penurunan ketajaman penglihatan, gangguan daya penglihatan.\r\n\r\n', 'Obat Keras', '1 Dos isi 10 Strip x 10 Tablet\r\n\r\n', 'TRIMAN'),
(5, 'Ciprofloxacin Dexa 500mg Tablet (per Tablet)', '852.00', 'CIPROFLOXACIN DEXA 500MG TAB merupakan antibiotik yang tergolong dalam kelas kuinolon, memiliki cara kerja menghentikan pertumbuhan bakteri. Ciprofloxacin digunakan untuk mengatasi infeksi bakteri seperti infeksi saluran kemih, infeksi saluran pernafasan, kulit dan jaringan lunak, tulang dan sendi, diare, infeksi menular seksual, anthraks.\r\n\r\n', 60, 'storage/image/ciprofloxacin.png', 'Tablet', 'Ciprofloxacin 500 mg\r\n\r\n', 'Infeksi saluran kemih, infeksi saluran pernafasan, infeksi menular seksual,infeksi pada bagian kulit dan jaringan lunak, tulang dan sendi, diare, anthraks\r\n\r\n', 'Infeksi Saluran Kemih (ISK): Ringan - sedang 250 mg 2 kali sehari, infeksi berat 500 mg 2 kali sehari. \r\nInfeksi Saluran Pernafasan, kulit dan jaringan lunak, tulang dan sendi: Ringan - sedang 250 - 500 mg 2 kali sehari, berat 500-750 mg 2 kali sehari. \r\nProstatitis kronis, infeksi saluran cerna : 500 mg 2 kali sehari. \r\nOsteomielitis akut : 750 mg 2 kali sehari. GO akut : 250 mg dosis tunggal. \r\nISK bagian atas dan bawah : 100 mg 2 kali sehari. \r\nInfeksi lain : 200 mg 2 kali sehari. \r\nGO akut dan sistitis akut tanpa komplikasi pada wanita : 100 mg dosis tunggal\r\n\r\n', 'Dapat diberikan bersama makan untuk mengurangi rasa tidak nyaman pada saluran pencernaan. Jangan diberikan bersama antasida, zat besi atau produk susu.\r\n\r\n', 'Mual, muntah, diare, dispepsia, nyeri abdomen, kembung, sakit kepala, lemah, insomnia, tremor.\r\n\r\n', 'Obat Keras\r\n\r\n', '1 Dos isi 10 Strip x 10 Tablet\r\n\r\n', 'DEXA MEDICA'),
(6, 'Panadol Biru Tablet 1 Strip (per Blister)', '13011.00', 'PANADOL BIRU TABLET 1 STRIP memiliki kandungan bahan aktif Paracetamol. Paracetamol bekerja dengan cara mengurangi kadar prostaglandin di dalam tubuh, sehingga tanda peradangan seperti demam dan nyeri akan berkurang. Obat ini digunakan untuk meringankan rasa sakit seperti sakit kepala, sakit gigi serta menurunkan demam.\r\n\r\nParacetamol dapat dikonsumsi sesudah makan. Konsultasikan terlebih dahulu kepada dokter apabila akan digunakan pada pasien dengan kondisi:\r\n• Riwayat alergi terhadap kandungan obat ini.\r\n• Pasien dengan penyakit gangguan hati berat.\r\n• Pasien yang mengonsumsi alkohol dapat meningkatkan potensi kerusakan hati.\r\n• Pasien dengan penyakit gangguan ginjal.\r\n\r\nInteraksi dapat terjadi apabila digunakan bersamaan dengan obat-obatan seperti: • Metoclopramide dapat meningkatkan efek analgetic paracetamol.\r\n• Paracetamol meningkatkan efek obat warfarin.\r\n• Carbamazepine meningkatkan potensi kerusakan hati.\r\n\r\n', 80, 'storage/image/paracetamol.png', 'Blister', 'Paracetamol 500 mg.\r\n\r\n', 'Meringankan rasa sakit seperti sakit kepala, sakit gigi dan menurunkan demam.\r\n\r\n', '• Dewasa: 1-2 tablet, diberikan sebanyak 3-4 kali per hari.\r\n• Anak usia 6-12 tahun: ½-1 tablet, diberikan sebanyak 3-4 kali per hari. \r\n\r\n', 'Dapat diberikan setelah makan.\r\n\r\n', 'Mual, muntah, nyeri lambung, kehilangan nafsu makan, ruam pada kulit.\r\n\r\n', 'Obat Bebas\r\n\r\n', '1 Dos isi 10 Blister x 10 Tablet\r\n\r\n', 'STERLING PRODUCTS INDONESIA'),
(7, 'Levothyroxine Novell 100mcg Tablet (per Tablet)', '1291.00', 'LEVOTHYROXINE NOVELL 100MCG TABLET memiliki kandungan bahan aktif Levothyroxine. Levothyroxine merupakan obat yang digunakan untuk mengobati hipotiroidisme atau kondisi ketika kelenjar tiroid hanya mampu menghasilkan sedikit hormon tiroid sehingga dapat menyebabkan terganggunya proses metabolisme tubuh.\r\nTiavell harus diminum saat perut kosong. Ambil 30 menit-1 jam sebelum makan. Konsultasikan terlebih dahulu kepada dokter apabila akan digunakan pada pasien dengan kondisi atau riwayat sakit:\r\n• Riwayat alergi terhadap kandungan obat ini.\r\n• Pasien dengan penyakit angina, gagal jantung, hipertensi.\r\n• Pasien dengan penyakit Diabetes Mellitus.\r\n\r\n', 1291, 'storage/image/levothyroxine.png', 'Tablet', 'Levothyroxine 100 mcg.\r\n\r\n', 'Mengobati hipotiroidisme.\r\n\r\n', 'Kondisi Hipotiroidisme\r\nDewasa: Dosis awal 50-100 mcg, 1 kali sehari. \r\n\r\n', 'Harus diminum saat perut kosong.\r\n\r\n', 'Gugup, rangsangan, tremor, kelemahan otot, kelelahan, kram berkeringat, kemerahan.\r\n\r\n', 'Obat Keras\r\n\r\n', '1 Dos isi 3 Strip x 10 Tablet\r\n\r\n', 'NOVELL PHARMACEUTICAL LABORATORIES'),
(8, 'Metformin Promed 750mg Kaplet (per Tablet)', '941.00', 'METFORMIN PROMED 750MG CAPL 100S termasuk golongan obat Biguanid yang bekerja dengan cara menekan produksi glukosa hati dan menambah sensitifitas terhadap insulin. Metformin digunakan untuk menurunkan kadar gula darah yang meningkat pada penderita diabetes. Penggunaan obat ini bisa sebagai obat tunggal atau dikombinasikan dengan obat penurun gula darah yang lainnya, sesuai dengan petunjuk dari dokter.\r\n\r\nMetformin dapat dikonsumsi bersamaan dengan makanan. Konsultasikan terlebih dahulu kepada dokter apabila akan digunakan pada pasien dengan kondisi atau riwayat sakit:\r\n• Riwayat alergi terhadap kandungan obat ini.\r\n• Pasien dengan gagal jantung kongestif.\r\n• Gangguan fungsi ginjal.\r\n\r\nInteraksi dapat terjadi apabila digunakan bersamaan dengan obat-obatan seperti:\r\n• Furosemid.\r\n• Nifedipine.\r\n\r\n', 90, 'storage/image/metformin.png', 'Tablet', 'Metformin 750 mg.\r\n\r\n', 'Penurun gula darah bagi penderita kencing manis (diabetes mellitus). Metformin juga sangat cocok untuk penderita diabetes dengan tubuh gemuk.\r\n\r\n', '• Dewasa:\r\nDosis awal: 500 mg/hari.\r\nDosis maksimal: 2.000 mg/hari.\r\n\r\n', 'Dapat dikonsumsi bersamaan dengan makanan.\r\n\r\n', 'Diare, banyak buang gas, rasa lelah, nyeri-nyeri otot, gula darah rendah (hipoglikemia), penurunan kadar vitamin B-12 dalam tubuh, sulit buang air besar, sakit maag.\r\n\r\n', 'Obat Keras\r\n\r\n', '1 Dos isi 10 Strip x 10 Tablet\r\n\r\n', 'PROMED'),
(9, 'Omeprazole Dexa 20mg Kapsul (per Tablet)', '755.00', 'OMEPRAZOLE DEXA 20MG CAP 100S termasuk obat golongan PPI (Proton Pump Inhibitor) dengan zat aktif Omeprazole yang digunakan untuk mengatasi penyakit-penyakit yang disebabkan oleh kelebihan produksi asam lambung, seperti sakit maag dan tukak lambung. Mekanisme kerja dari Omeprazole yaitu menghambat sekresi asam lambung melalui sistem enzim adenosin trifosfatase hidrogen-kalium (pompa proton) dari sel parietal lambung. Obat ini dapat dikonsumsi sebelum makan. Apabila lupa untuk mengosumsi omeprazole segera konsumsi dosis yang terlupakan begitu ingat. Jangan mengonsumsi obat jika sudah dekat dengan waktu dosis berikutnya. Konsultasikan kepada dokter jika memiliki riwayat alergi terhadap obat omeprazole. Interaksi obat dapat terjadi apabila digunakan secara bersamaan dengan obat: *Diazepam *Fenitoin *Warfarin *Ketokonazol\r\n\r\n', 110, 'storage/image/omeprazole.png', 'Tablet', 'Omeprazole 20 mg\r\n\r\n', 'Tukak duodenum atau lambung, esofagitis ulseratif, sindrom Zollinger-Ellison.\r\n\r\n', 'Dapat diberikan sebelum makan.\r\n\r\n', '*Refluks esofagitis: 20 mg 1x/hari selama 4-8 minggu, kasus berat: 40 mg 1x/hari selama 8 minggu. *Tukak duodenum: 20 mg 1x/hari selama 2-4 minggu.\r\n\r\n', 'Jarang terjadi: *Gangguan GI *Sakit kepala *Ruam kulit\r\n\r\n', 'Obat Keras\r\n\r\n', '1 Dos isi 10 Strip x 10 Tablet\r\n\r\n', 'DEXA'),
(10, 'Prednison Triman 5mg Tablet (per Tablet)', '213.00', 'PREDNISON TRIMAN 5MG TAB 100S termasuk dalam kelompok kortikosteroid sintetis atau lebih dikenal dengan steroid. Obat ini bekerja dengan cara mencegah pelepasan substansi dalam tubuh yang menyebabkan inflamasi. Prednison meniru efek kortisol, hormon yang dilepaskan oleh kelenjar adrenal (terletak di atas ginjal) yang mengontrol metabolisme dan stres.\r\n\r\nPrednison digunakan sebagai anti alergi, imunosupresan, dan anti inflamasi. Dapat dikonsumsi bersamaan dengan makan. Konsultasikan terlebih dahulu kepada dokter apabila akan digunakan pada pasien dengan riwayat atau kondisi sakit seperti:\r\n\r\n• Riwayat alergi terhadap kandungan obat ini.\r\n• Penyakit gastrointestinal (mis. ulkus peptikum aktif atau laten, kolitis ulseratif nonspesifik, divertikulitis, anastomosis usus segar).\r\n• Epilepsi atau riwayat gangguan kejang.\r\n• Hipertensi.\r\n\r\nInteraksi dapat terjadi jika dikonsumsi dengan obat-obatan lain, seperti:\r\n\r\n• Rifampin: Penurunan efektivitas prednison.\r\n• Phenytoin: Penurunan efektivitas prednison.\r\n• Hormon Estrogen: Peningkatan efektivitas prednison.\r\n\r\n', 70, 'storage/image/prednisone.png', 'Tablet', 'Prednison 5 mg.\r\n\r\n', 'Menekan reaksi radang dan reaksi alergi.\r\n\r\n', '• Antiinflamasi atau imunosupresif.\r\nDewasa: 5-60 mg setiap hari.\r\nAnak-anak: 0,05-2 mg / kg setiap hari dibagi setiap 6-24 jam.\r\n\r\n• Artritis reumatoid.\r\nDewasa: 5-10 mg pada waktu tidur, dapat disesuaikan sesuai dengan respons pasien, gejala klinis, dan tingkat keparahan penyakit. Dosis dapat dikurangi dengan penambahan 1 mg setiap 2-4 minggu sampai dosis pemeliharaan yang tepat tercapai.\r\n\r\n', 'Dapat dikonsumsi bersamaan dengan makan.\r\n\r\n', 'Mual, anoreksia (kehilangan nafsu makan), nyeri otot,gelisah. Edema, hipernatremia, hipokalemia, iritasi lambung, hipernatremia, hiperkalemia. Gangguan tidur (pada awal terapi).\r\n\r\n', 'Obat Keras\r\n\r\n', '1 Dos isi 10 Strip x 10 Tablet\r\n\r\n', 'TRIMAN'),
(11, 'Simvastatin Errita 10mg Fc Tablet (per Tablet)', '262.00', 'SIMVASTATIN ERRITA 10MG FC TAB 100S termasuk obat golongan inhibitor reductase HMG CoA atau statin yang bekerja dengan cara menghambat enzim pembentuk kolesterol jahat sehingga kadar kolesterol dalam darah berkurang. Simvastatin mengurangi kadar kolesterol buruk (LDL) dan trigliserid dalam darah, sekaligus menaikkan kadar HDL (kolesterol baik) serta digunakan untuk menurunkan risiko kematian karena penyakit jantung koroner, menurunkan risiko infark miokard non fatal, menurunkan risiko pada pasien yang sedang menjalani tindakan revaskularisasi miokardial.\r\n\r\nSimvastatin dapat dikonsumsi sebelum atau sesudah makan pada malam hari, biasanya satu kali setiap hari di waktu malam sebelum tidur atau sesuai yang disarankan oleh dokter. Konsultasikan terlebih dahulu kepada dokter apabila akan digunakan pada pasien dengan riwayat atau kondisi sakit seperti:\r\n• Riwayat alergi terhadap kandungan obat ini.\r\n• Pasien dengan penyakit hati aktif atau peningkatan persisten transaminase serum.\r\n• Pasien dengan penyakit ginjal dan penyakit tiroid.\r\n\r\nInteraksi dapat terjadi apabila digunakan bersamaan dengan obat-obatan seperti:\r\n• Imunosupresan.\r\n• Itrakonazol.\r\n• Gemfibrozil.\r\n• Niasin.\r\n• Eritromisin.\r\n• Kumarin.\r\n• Antipirin.\r\n• Propanolol.\r\n• Digoksin.\r\n\r\nInteraksi simvastatin dan makanan dapat terjadi terutama dengan grapefruit (limau gedang). Buah ini dapat meningkatkan penyerapan obat dalam aliran darah.\r\n\r\n', 30, 'storage/image/simvastatin.png', 'Tablet', 'Simvastatin 10 mg.\r\n\r\n', 'Menurunkan kadar kolestrol total & LDL pada pasien dengan hiperkolesterolemia primer (tipe IIa & IIb).\r\n\r\n', 'Awal: 5-10 mg 1 kali sehari (malam hari). Maksimal 40 mg/hari sebagai dosis tunggal.\r\n\r\n', 'Dapat diberikan sebelum atau sesudah makan pada malam hari.\r\n\r\n', 'Nyeri perut, konstipasi, kembung, astenia, sakit kepala, miopati, rabdomiolisis, tremor, pusing, vertigo.\r\n\r\n', 'Obat Keras\r\n\r\n', '1 Dos isi 10 Strip x 10 Tablet\r\n\r\n', 'ERRITA PHARMA'),
(12, 'OBH Tropica Plus Anak Straw Sirup 60ml (per Botol)', '25000.00', 'OBH TROPICA PLUS ANAK STRAW SYR 60ML merupakan obat sirup rasa strawberry yang digunakan untuk meredakan batuk, sakit kepala, hidung tersumbat, demam karena influenza. Tiap 5 ml OBH TROPICA PLUS ANAK STRAW SYR 100ML syrup mengandung: *Ammonium chloride bersifat sebagai agen ekspektoran dalam obat batuk. Efek ekspektoran akan bekerja dengan cara mengiritasi mukosa bronkial yang mempermudah pengeluaran dahak. *Chlorpheniramine maleate merupakan obat alergi golongan antihistamin generasi pertama. *Pseudoephedrine hydrochloride merupakan obat amina simpatomimetik, dalam sediaan obat ini bertindak sebagai nasal dekongestan. *Glycyrrhizae succus merupakan salah satu komponen obat batuk hitam (OBH) yang diperoleh dari sari akar tanaman Glycyrrhiza glabra varietas glandulifera dan memberikan efek ekspektoran. *Paracetamol obat yang memiliki aktivitas sebagai anti nyeri dan penurun demam. OBH TROPICA PLUS ANAK STRAW SYR 100ML dapat dikonsumsi sesudah makan. Konsultasikan kepada dokter terlebih dahulu apabila akan digunakan pada pasien dengan kondisi: *Riwayat alergi terhadap salah satu komponen obat ini *Pasien penderita hipertensi parah *Penyakit jantung *Diabetes mellitus *Gangguan fungsi hati Interaksi dapat terjadi apabila digunakan bersamaan dengan obat-obatan seperti: *Metoclopramide dapat meningkatkan efek analgetic paracetamol. *Paracetamol meningkatkan efek obat warfarin. *Carbamazepine meningkatkan potensi kerusakan hati. Beli OBH TROPICA PLUS ANAK STRAW SYR 100ML di K24Klik dan dapatkan manfaatnya.\r\n\r\n', 85, 'storage/image/OBH.png', 'Botol', '*Glycyrrhizae succus *Paracetamol *Ammonium chloride *Pseudoephedrine hydrochloride *Chlorpheniramine maleate\r\n\r\n\r\n\r\n', 'Mengatasi gejala flu seperti demam, sakit kepala, hidung tersumbat dan bersin-bersin yang disertai batuk.\r\n\r\n', 'Dapat diberikan sesudah makan.\r\n\r\n', '*Dosis anak usia 2-5 tahun : 3 x sehari 1 sendok teh. *Anak usia 6-12 tahun : 3 x sehari 2 sendok teh. *Keterangan: sendok teh = 5 ml\r\n\r\n', 'Mual, muntah, sakit kepala, mengantuk, vertigo, takikardi, mulut kering.\r\n\r\n', 'Obat Bebas Terbatas\r\n\r\n', '1 Botol\r\n\r\n', 'Tropica Mas Pharmaceutical');
